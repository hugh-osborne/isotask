<Simulation>
<WeightType>CustomConnectionParameters</WeightType>
<Algorithms>
<Algorithm type="GridAlgorithm" name="INTERNEURON" modelfile="int.model" tau_refractive="0.0" transformfile="int_0_0_0_0_.tmat" start_v="-64.0" start_w="0.6" ratemethod="AvgV">
<TimeStep>0.000001</TimeStep>
</Algorithm>
<Algorithm type="GridSomaDendriteAlgorithm" name="SOMA" modelfile="soma.model" tau_refractive="0.0" transformfile="soma_0_0_0_0_.tmat" start_v="-0.5" start_w="0.0000001" >
<TimeStep>0.000001</TimeStep>
</Algorithm>
<Algorithm type="GridSomaDendriteAlgorithm" name="DENDRITE" modelfile="dendrite.model" tau_refractive="0.0" transformfile="dendrite_0_0_0_0_.tmat" start_v="-0.5" start_w="0.0000001" >
<TimeStep>0.000001</TimeStep>
</Algorithm>
</Algorithms>
<Nodes>
<Node algorithm="INTERNEURON" name="INT_AG" type="NEUTRAL" />
<Node algorithm="INTERNEURON" name="INT_ANTAG" type="NEUTRAL" />

<Node algorithm="SOMA" name="MN_RF" type="NEUTRAL" />
<Node algorithm="DENDRITE" name="MN_RF_DEND" type="NEUTRAL" />
<Node algorithm="SOMA" name="MN_VL" type="NEUTRAL" />
<Node algorithm="DENDRITE" name="MN_VL_DEND" type="NEUTRAL" />
<Node algorithm="SOMA" name="MN_VM" type="NEUTRAL" />
<Node algorithm="DENDRITE" name="MN_VM_DEND" type="NEUTRAL" />

<Node algorithm="SOMA" name="MN_ST" type="NEUTRAL" />
<Node algorithm="DENDRITE" name="MN_ST_DEND" type="NEUTRAL" />
<Node algorithm="SOMA" name="MN_BF" type="NEUTRAL" />
<Node algorithm="DENDRITE" name="MN_BF_DEND" type="NEUTRAL" />
</Nodes>
<Connections>

<Connection In="MN_RF" Out="MN_RF_DEND" type="SomaDendrite" num_connections="1000" efficacy="1.0" delay="0.0" conductance="0.8"/>
<Connection In="MN_RF_DEND" Out="MN_RF" type="SomaDendrite" num_connections="1000" efficacy="1.0" delay="0.0" conductance="0.8"/>

<Connection In="MN_VL" Out="MN_VL_DEND" type="SomaDendrite" num_connections="1000" efficacy="1.0" delay="0.0" conductance="0.8"/>
