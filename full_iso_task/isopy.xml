<OutgoingConnection Node="MN_RF"/>
<OutgoingConnection Node="MN_VL"/>
<OutgoingConnection Node="MN_VM"/>
<OutgoingConnection Node="MN_ST"/>
<OutgoingConnection Node="MN_BF"/>
</Connections>
