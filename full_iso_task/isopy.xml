<Connection In="MN_VM" Out="MN_VM_DEND" type="SomaDendrite" num_connections="1000" efficacy="1.0" delay="0.0" conductance="0.8"/>
<Connection In="MN_VM_DEND" Out="MN_VM" type="SomaDendrite" num_connections="1000" efficacy="1.0" delay="0.0" conductance="0.8"/>

<Connection In="MN_ST" Out="MN_ST_DEND" type="SomaDendrite" num_connections="1000" efficacy="1.0" delay="0.0" conductance="0.8"/>
<Connection In="MN_ST_DEND" Out="MN_ST" type="SomaDendrite" num_connections="1000" efficacy="1.0" delay="0.0" conductance="0.8"/>

<Connection In="MN_BF" Out="MN_BF_DEND" type="SomaDendrite" num_connections="1000" efficacy="1.0" delay="0.0" conductance="0.8"/>
<Connection In="MN_BF_DEND" Out="MN_BF" type="SomaDendrite" num_connections="1000" efficacy="1.0" delay="0.0" conductance="0.8"/>

<Connection In="INT_AG" Out="MN_ST" type="input" avgv_offset="64.0" num_connections="200" efficacy="-0.01" delay="0.0" conductance="0.0"/>
<Connection In="INT_AG" Out="MN_BF" type="input" avgv_offset="64.0" num_connections="200" efficacy="-0.01" delay="0.0" conductance="0.0"/>

<Connection In="INT_ANTAG" Out="MN_RF" type="input" avgv_offset="64.0" num_connections="200" efficacy="-0.01" delay="0.0" conductance="0.0"/>
<Connection In="INT_ANTAG" Out="MN_VL" type="input" avgv_offset="64.0" num_connections="200" efficacy="-0.01" delay="0.0" conductance="0.0"/>
<Connection In="INT_ANTAG" Out="MN_VM" type="input" avgv_offset="64.0" num_connections="200" efficacy="-0.01" delay="0.0" conductance="0.0"/>

